<launch>
  <node pkg="tf" type="static_transform_publisher" name="base_to_imu_link" args="0 0 0 0 0 0 /base_link /imu_link 1000" />
  <node pkg="tf" type="static_transform_publisher" name="base_to_car_odom" args="0 0 0 0 0 0 /base_link /car_odom 1000" />
  
  <node pkg="xj_robot_slam" type="change_odom_id.py" name="change_odom_id" />

  <node pkg="robot_localization" type="ukf_localization_node" name="ukf_se" clear_params="true">
    <rosparam command="load" file="$(find xj_robot_slam)/ekf_params/ukf_template.yaml" />

    <!--  Placeholder for output topic remapping
    <remap from="odometry/filtered" to=""/>
    <remap from="accel/filtered" to=""/>
    -->

  </node>
</launch>
